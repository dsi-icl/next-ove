  "scripts": {
    "analyze-bundle": "npm run analyze-bundle:bridge && npm run analyze-bundle:client && npm run analyze-bundle:core",
    "analyze-bundle:bridge": "cd apps/ove-bridge-ui && npx vite-bundle-visualizer --open=false --output=../../tools/bundle-analysis/ove-bridge.html && npx rimraf dist",
    "analyze-bundle:client": "cd apps/ove-client-ui && npx vite-bundle-visualizer --open=false --output=../../tools/bundle-analysis/ove-client.html && npx rimraf dist",
    "analyze-bundle:core": "cd apps/ove-core-ui && npx vite-bundle-visualizer --open=false --output=../../tools/bundle-analysis/ove-core.html && npx rimraf dist",
    "audit": "npm audit > tools/package-audit/analysis.txt",
    "build": "npx nx run-many --target=build",
    "clean": "npx rimraf dist",
    "css-usage": "node tools/doiuse/doiuse.js tools/doiuse/browser-usage.json",
    "analyze-deprecation": "tools/aqua-nautilus/analyze.sh",
    "jsdoc": "npx jsdoc . -c tools/jsdoc/jsdoc.json",
    "lhci:audit": "node tools/.lighthouseci/audit.js tools/.lighthouseci/config.json",
    "lhci:wizard": "npx lhci wizard",
    "lhci:server": "npx lhci server --config=tools/.lighthouseci/lighthouserc.json --port=9002 --storage.storageMethod=sql --storage.sqlDialect=sqlite --storage.sqlDatabasePath=./tools/.lighthouseci/db.sql",
    "lint": "npx nx run-many --target=lint",
    "list": "npm ls --all --json --silent > tools/packages/packages.json && npm ls --all --silent > tools/packages/packages.txt",
    "package-audit": "npx sandworm-audit -o tools/sandworm",
    "prisma": "cd tools/db && npx prisma generate",
    "prisma:pull": "cd tools/db && npx prisma db pull",
    "prisma:push": "cd tools/db && npx prisma db push",
    "start": "npx nx run-many --target=serve",
    "test:integration": "npx jest --config jest.integration.config.ts",
    "test": "npx nx run-many --target=test",
    "ts-coverage": "npx typescript-coverage-report -o tools/coverage-ts",
    "typedoc": "npx typedoc --options tools/typedoc/typedoc.json --tsconfig tsconfig.json",
    "user:add": "node tools/db/add-user.js"
  },
