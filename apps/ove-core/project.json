{
  "name": "ove-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ove-core/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/ove-core",
        "main": "apps/ove-core/src/main.ts",
        "tsConfig": "apps/ove-core/tsconfig.app.json",
        "assets": ["apps/ove-core/src/assets", "apps/ove-core/src/config"],
        "webpackConfig": "apps/ove-core/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/ove-core/src/environments/environment.ts",
              "with": "apps/ove-core/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "ove-core:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "ove-core:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/ove-core/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/ove-core/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
